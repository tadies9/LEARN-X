{
  "name": "backend",
  "version": "1.0.0",
  "description": "LEARN-X Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:api": "nodemon src/api-server.ts",
    "dev:worker": "nodemon src/workers/enhanced-pgmq-worker.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:debug": "node dist/debug-start.js",
    "start:api": "node dist/api-server.js",
    "start:worker": "node dist/workers/enhanced-pgmq-worker.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit -p tsconfig.build.json",
    "type-check:all": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart backend",
    "init:pgmq": "node scripts/init-pgmq.js",
    "embeddings:status": "node scripts/check-embeddings.js",
    "system:health": "node scripts/check-embeddings.js health",
    "production:init": "npm run init:pgmq",
    "production:migrate": "node scripts/run-enhanced-pgmq-migration.js",
    "production:deploy": "npm run build && npm run production:migrate",
    "test:system": "node test-enhanced-pipeline.js",
    "queue:health": "node -e \"import('./dist/services/queue/QueueOrchestrator.js').then(m => m.queueOrchestrator.getSystemHealth()).then(console.log)\"",
    "queue:purge": "node -e \"import('./dist/services/queue/QueueOrchestrator.js').then(m => m.queueOrchestrator.emergencyPurgeAllQueues())\"",
    "monitor:queues": "watch -n 5 'curl -s http://localhost:8080/health/queues | jq'",
    "monitor:performance": "watch -n 10 'curl -s http://localhost:8080/health/performance | jq'"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@types/multer": "^1.4.13",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "ipaddr.js": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "openai": "^4.24.1",
    "pdf-parse": "^1.1.1",
    "tiktoken": "^1.0.21",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
